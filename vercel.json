{
    "version": 2,
    "rewrites": [
        {
            "use": "@vercel/node",
            "config": {
                "includeFiles": [
                    "dist/**"
                ]
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "server.js",
            "methods" : ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"]
        }
    ]
}
